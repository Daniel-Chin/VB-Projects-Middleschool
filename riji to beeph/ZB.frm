Attribute VB_Name = "Form1"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit

Private Declare Function SetWindowLongA Lib "user32" (ByVal hwnd As Long, ByVal nIndex As Long, ByVal dwNewLong As Long) As Long
Private Declare Function SetLayeredWindowAttributes Lib "user32" (ByVal hwnd As Long, ByVal crKey As Long, ByVal bAlpha As Byte, ByVal dwFlags As Long) As Long
Private Declare Function SetWindowPos& Lib "user32" (ByVal hwnd As Long, ByVal hWndInsertAfter As Long, ByVal X As Long, ByVal Y As Long, ByVal cx As Long, ByVal cy As Long, ByVal wFlags As Long)

Dim Last As Integer

Private Sub Form_Load()
Tag = SetWindowPos(Me.hwnd, -1, 0, 0, 0, 0, 3) '-1 Do it,-2 Reset
    SetWindowLongA hwnd, -20, 524288
    SetLayeredWindowAttributes hwnd, 0, 200, 2
Dim i As Integer
For i = 0 To 25
    B(i).Caption = Chr(65 + i)
Next i
Move Screen.Width - Me.Width, Screen.Height - Me.Height
End Sub

Public Sub Activate(T As String)
Dim i As Integer
Do Until B(i).Caption = StrConv(T, vbUpperCase)
    i = i + 1
    If i = 26 Then Exit Do
Loop
If i < 26 Then
    B(Last).BackColor = Me.BackColor
    B(Last).ForeColor = Me.ForeColor
    Last = i
    B(Last).BackColor = Me.ForeColor
    B(Last).ForeColor = Me.BackColor
    DoEvents
End If
End Sub
