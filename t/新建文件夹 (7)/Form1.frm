Private Sub Form_Load()
Show
Me.BackColor = vbBlue
PaintPicture LoadPicture(App.Path & "\qrcode.bmp"), 0, 0
For x = 0 To 40
For y = 0 To 40
    If Point(x, y) > 1 Then
        S(x, y) = True
    End If
Next y, x
Stop
Cls
sc = 10
For x = 1 To 41
For y = 1 To 41
    If Not S(x - 1, y - 1) Then
        Me.FillColor = 0
    Else
        Me.FillColor = vbWhite
    End If
    Me.ForeColor = Me.FillColor
    Line (x * sc, y * sc)-((x + 1) * sc, (y + 1) * sc), , B
Next y, x
For x = 0 To 40
Debug.Print "QR(" & x & ") = array(";
For y = 0 To 40
    If S(x, y) Then
        '是白的
        Debug.Print "1,";
    Else
        Debug.Print "0,";
    End If
Next y
Debug.Print ")"
Next x
End Sub

